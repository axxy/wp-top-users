{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "ahmedyahya/top-users-block",
	"version": "0.1.0",
	"title": "Top Users Block",
	"category": "widgets",
	"icon": "groups",
	"description": "Display top users based on total order value.",
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "top-users-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"attributes": {
		"order": {
			"type": "string",
			"default": "desc"
		},
		"numberOfUsers": {
			"type": "number",
			"default": 5
		}
	}
}
